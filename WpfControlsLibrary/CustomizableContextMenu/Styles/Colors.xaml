<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfControlsLibrary.CustomizableContextMenu.Styles">

    <SolidColorBrush x:Key="brushContour" Color="#FF969FA8"></SolidColorBrush>
    <SolidColorBrush x:Key="brushMainBackground" Color="#FF4E565F"></SolidColorBrush>
    <SolidColorBrush x:Key="brushSecondBackground" Color="#FF818181"></SolidColorBrush>
    <SolidColorBrush x:Key="brushButtonHighlightBorder" Color="#FFC4D3E8"></SolidColorBrush>
    <SolidColorBrush x:Key="brushText" Color="#FFE6ECF5"></SolidColorBrush>
    <SolidColorBrush x:Key="brushUpperButton" Color="#FF353B43"></SolidColorBrush>

    <LinearGradientBrush x:Key="brushNormalButton" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Offset="0" Color="#FFFFFFFF"></GradientStop>
        <GradientStop Offset="1" Color="#FFC3D2E7"></GradientStop> 
    </LinearGradientBrush>
    
</ResourceDictionary>